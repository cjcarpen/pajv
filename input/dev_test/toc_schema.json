                                                        "properties": {
                                                            "label": { "type": "string" },
                                                            "topics": { 
                                                                "type": "array",
                                                                "items": {
                                                                    "type": "string"
                                                                }
                                                            }
                                                        }
                                                    }
                                                }
                                            }
                                        ]
                                    }
                                }
                            }
                        }
                    } 
                }
            },
            "required": ["props", "entries"]
        }
    }
}